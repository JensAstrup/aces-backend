{
  "name": "aces-backend",
  "version": "1.0.0-alpha.16",
  "scripts": {
    "build-base": "npx ts-node build.ts",
    "build": "NODE_ENV=production npm run build-base && npm run sentry:sourcemaps && npm run cleanup:sourcemaps",
    "lint": "npx eslint --ext .ts src/ tests/",
    "lint:tests": "npx eslint --ext .ts tests/",
    "start": "yarn prisma:deploy && node -r module-alias/register ./dist --env=production",
    "dev": "dotenvx run -f ./env/development.env -- nodemon",
    "test": "jest --env-file=./env/test.env",
    "test:coverage": "node --env-file ./env/test.env node_modules/.bin/jest --coverage --coverageDirectory=coverage",
    "prisma:deploy": "npx prisma migrate deploy",
    "sentry:new-release": "sentry-cli releases new --project ace-of-spades-backend $npm_package_version",
    "sentry:inject": "sentry-cli sourcemaps inject --org jens-astrup --project ace-of-spades-backend ./dist",
    "sentry:upload": "sentry-cli sourcemaps upload --org jens-astrup --project ace-of-spades-backend --release=$npm_package_version ./dist",
    "sentry:finalize": "sentry-cli releases finalize $npm_package_version",
    "sentry:sourcemaps": "npm run sentry:new-release && npm run sentry:inject && npm run sentry:upload && npm run sentry:finalize",
    "cleanup:sourcemaps": "find ./dist -name '*.map' -type f -delete"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@aces": "dist"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.14.2",
    "@linear/sdk": "^30.0.0",
    "@prisma/client": "5.17.0",
    "@sentry/cli": "^2.36.5",
    "@sentry/node": "^8.32.0",
    "@sentry/profiling-node": "^8.25.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csrf-sync": "^4.0.3",
    "express": "^4.20.0",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "inserturlparams": "^2.0.4",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "qs": "^6.13.0",
    "ts-command-line-args": "^2.5.1",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/jest": "^29.5.12",
    "@types/jsonfile": "^6.1.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-yenz": "^6.1.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.4.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
