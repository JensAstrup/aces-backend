{
  "name": "aces-backend",
  "version": "1.0.0-alpha.8",
  "scripts": {
    "build": "npx ts-node build.ts",
    "lint": "npx eslint --ext .ts src/ tests/",
    "lint:tests": "npx eslint --ext .ts tests/",
    "start": "npm run prisma:deploy && node -r module-alias/register ./dist --env=production",
    "dev": "dotenvx run -f ./env/development.env -- nodemon",
    "test": "jest --env-file=./env/test.env",
    "test:coverage": "node --env-file ./env/test.env node_modules/.bin/jest --coverage --coverageDirectory=coverage",
    "prisma:deploy": "npx prisma migrate deploy"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@aces": "dist"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.6.4",
    "@linear/sdk": "^24.0.0",
    "@prisma/client": "5.17.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "inserturlparams": "^2.0.2",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "qs": "^6.12.3",
    "ts-command-line-args": "^2.5.1",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/jest": "^29.5.12",
    "@types/jsonfile": "^6.1.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-yenz": "^5.7.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prisma": "^5.17.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-jest": "^29.2.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
